{% extends 'base.html.twig' %}

{% block layout %}
    <h1>Resourcetheme</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ resourceTheme.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ resourceTheme.name }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('resourcetheme_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('resourcetheme_edit', { 'id': resourceTheme.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
